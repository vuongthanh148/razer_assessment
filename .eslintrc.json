{
  "env": {
    "node": true,
    "jest": true
  },
  "extends": [],
  "plugins": ["security", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "func-names": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "jest/expect-expect": "off",
    "security/detect-object-injection": "off",
    "no-magic-numbers": "warn"
  }
}
